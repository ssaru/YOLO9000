{
    "name": "YOLO9000",
    "n_gpu": 1,
    
    "arch": {
        "type": "Yolo9000",
        "args": {}
    },
    "data_loader": {
        "type": "DetectionDataLoader",
        "args":{
            "data_dir": "/home/martin/Documents/dev/Deepbaksu_vision/Datasets/VOCdevkit/VOC2007/",
            "batch_size": 32,
            "shuffle": true,
            "validation_split": 0.1,
            "num_workers": 4
        }
    },
    "optimizer": {
        "type": "Adam",
        "args":{
            "lr": 0.001,
            "weight_decay": 0,
            "amsgrad": true
        }
    },
    "loss": "Yolo9000Loss",
    "metrics": [
        "my_metric"
    ],
    "lr_scheduler": {
        "type": "StepLR",
        "args": {
            "step_size": 50,
            "gamma": 0.1
        }
    },
    "trainer": {
        "epochs": 1000,
        "save_dir": "saved/",
        "save_period": 1,
        "verbosity": 2,
        
        "monitor": "min val_loss",
        "early_stop": 10,
        
        "tensorboardX": true,
        "log_dir": "saved/runs"
    }
}
