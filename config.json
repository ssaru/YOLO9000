{
    "name": "YOLO9000",
    "n_gpu": 1,
    
    "arch": {
        "type": "Yolo9000",
        "args": {
            "prior_boxes": [[0.0815404648553137, 0.13804731644524953], [0.8935830875249917, 0.9072204690364727], [0.22066863986723892, 0.3760369847076882], [0.43006069490404647, 0.6020577522476821], [0.6722030591603478, 0.7508179123598757]]
        }
    },
    "data_loader": {
        "type": "DetectionDataLoader",
        "args":{
            "data_dir": "/home/martin/Documents/dev/Deepbaksu_vision/_Datasets/VOC2012/",
            "batch_size": 32,
            "shuffle": true,
            "validation_split": 0.1,
            "num_workers": 4,
            "output_shape": [125, 13, 13]

        }
    },
    "optimizer": {
        "type": "Adam",
        "args":{
            "lr": 0.001,
            "weight_decay": 0,
            "amsgrad": true
        }
    },
    "loss": "Loss",
    "metrics": [
        "detection_metric"
    ],
    "lr_scheduler": {
        "type": "StepLR",
        "args": {
            "step_size": 50,
            "gamma": 0.1
        }
    },
    "trainer": {
        "epochs": 1000,
        "save_dir": "saved/",
        "save_period": 1,
        "verbosity": 2,
        
        "monitor": "min val_loss",
        "early_stop": 10,
        
        "tensorboardX": true,
        "log_dir": "saved/runs"
    }
}
