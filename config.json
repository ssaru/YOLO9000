{
    "name": "YOLO9000",
    "n_gpu": 1,
    
    "arch": {
        "type": "Yolo9000",
        "args": {
        }
    },
    "data_loader": {
        "type": "DetectionDataLoader",
        "args":{
            "data_dir": "/home/keti-1080ti/Documents/dev/Deepbaksu_vision/_Datasets/VOCdevkit/VOC2012",
            "batch_size": 32,
            "shuffle": true,
            "validation_split": 0.1,
            "num_workers": 0,
            "output_shape": [125, 13, 13]
        }
    },
    "optimizer": {
        "type": "Adam",
        "args":{
            "lr": 0.001,
            "weight_decay": 0,
            "amsgrad": true
        }
    },
    "loss": "DetectionLoss",

    "lr_scheduler": {
        "type": "StepLR",
        "args": {
            "step_size": 50,
            "gamma": 0.1
        }
    },
    "trainer": {
        "epochs": 1000,
        "save_dir": "saved/",
        "save_period": 100,
        "verbosity": 2,
        
        "monitor": "off",
        "early_stop": 10,
        
        "tensorboardX": true,
        "log_dir": "saved/runs"
    }
}
